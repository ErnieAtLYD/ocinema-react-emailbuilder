{
  "name": "ocinema-react-emailbuilder",
  "version": "0.1.0",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.5.1",
    "@researchgate/babel-plugin-transform-scss-import-to-string": "^0.1.3",
    "foundation-emails": "^2.2.1",
    "immer": "^5.0.0",
    "inline-css": "^2.4.2",
    "juice": "^5.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "quill-blot-formatter": "^1.0.5",
    "react": "^16.9.0",
    "react-api-cache": "^1.1.0",
    "react-dnd": "^9.4.0",
    "react-dnd-html5-backend": "^9.4.0",
    "react-dom": "^16.9.0",
    "react-inky": "^0.4.0",
    "react-quill": "^1.3.3",
    "react-redux": "^7.1.1",
    "react-scripts": "3.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "siphon-media-query": "^1.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "@researchgate/babel-plugin-transform-scss-import-to-string"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 90,
    "bracketSpacing": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@testing-library/dom": "^6.10.1",
    "@testing-library/react": "^9.3.2",
    "babel-jest": "^24.9.0",
    "css-modules-flow-types-loader": "^1.3.0",
    "customize-cra": "^0.8.0",
    "cypress": "^3.6.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "flow-bin": "^0.112.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.4",
    "react-dnd-cjs": "^9.4.0",
    "react-dnd-test-backend": "^9.4.0",
    "react-dnd-test-utils": "^9.4.0",
    "react-test-renderer": "^16.12.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "moduleNameMapper": {
      "^dnd-core$": "dnd-core/dist/cjs",
      "^react-dnd$": "react-dnd/dist/cjs",
      "^react-dnd-html5-backend$": "react-dnd-html5-backend/dist/cjs",
      "^react-dnd-test-backend$": "react-dnd-test-backend/dist/cjs",
      "^react-dnd-test-utils$": "react-dnd-test-utils/dist/cjs"
    }
  }
}
